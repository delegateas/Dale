# top-most EditorConfig file
root = true
[*]
encoding = utf-8
end_of_line = lf

[*.{fs,fsi,fsx}]
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true

[*.{cs,csx}]
charset = utf-8
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[{*.json,.travis.yml,*.sh}]
indent_style = space
indent_size = 2